PORT=5000
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/zenvoa-technologies?retryWrites=true&w=majority

# Email Configuration (Gmail SMTP)
